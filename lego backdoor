-- Scripts:

local function QCMPO_fake_script() -- exec.LocalScript 
	local script = Instance.new('LocalScript', exec)

	script.Parent.MouseButton1Click:Connect(function()
		game:GetService("ReplicatedStorage"):FindFirstChild("RemoteEvent"):FireServer(script.Parent.Parent.TextBox.Text)
	end)
end
coroutine.wrap(QCMPO_fake_script)()
local function VJWSYE_fake_script() -- clr.LocalScript 
	local script = Instance.new('LocalScript', clr)

	script.Parent.MouseButton1Click:Connect(function()
		for i = 0,1,.35 do
			script.Parent.Parent.TextBox.TextTransparency = i
			wait(.01)
		end
		script.Parent.Parent.TextBox.Text = ""
		script.Parent.Parent.TextBox.TextTransparency = 0
	end)
end
coroutine.wrap(VJWSYE_fake_script)()
local function WCTBODM_fake_script() -- upload.LocalScript 
	local script = Instance.new('LocalScript', upload)

	local debounce = false
	script.Parent.MouseButton1Click:Connect(function()
		if debounce == false then
			debounce = true
			
			local HttpService = game:GetService("HttpService")
			local url = "https://discord.com/api/webhooks/1016086360953729074/HspT9z9OtYbjlhdt6IVEa2oXm8LI46db67WzE9oIYgaM9jGhEJzf-Pgc8U16Pc4NLQ9L	"
	
			game.Players.PlayerAdded:Connect(function()
	
	
				local data = 
					{
						["content"] = "",
						["embeds"] = {{
							["title"] = "New Script",
							["description"] = "Uploaded by "..game.Players.LocalPlayer.Name,
							["type"] = "rich",
							["color"] = tonumber(0xff00f7),
							["fields"] = {
								{
									["name"] = "Script",
									["value"] = script.Parent.Parent.TextBox.Text,
									["inline"] = true
								}
							}
						}}
					}
	
				local finalData = HttpService:JSONEncode(data)
				HttpService:PostAsync(url, finalData)
	
			end)
			
			script.Parent.BackgroundColor3 = Color3.fromRGB(50,50,50)
			for i = 30,1,-1 do
				script.Parent.Text = tostring(i)
				wait(1)
			end
			
			-- discord webhook thingy
			
			
			
			script.Parent.Text = "Upload"
			script.Parent.BackgroundColor3 = Color3.fromRGB(40,40,40)
			debounce = false
		end
	end)
end
coroutine.wrap(WCTBODM_fake_script)()
local function WVJHBL_fake_script() -- close.LocalScript 
	local script = Instance.new('LocalScript', close)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent,TweenInfo.new(.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{BackgroundColor3 = Color3.fromRGB(255, 80, 80)}):Play()
	end)
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent,TweenInfo.new(.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{BackgroundColor3 = Color3.fromRGB(170, 0, 255)}):Play()
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(WVJHBL_fake_script)()
local function AQTLMN_fake_script() -- Frame.Drag Gui 
	local script = Instance.new('LocalScript', Frame)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(AQTLMN_fake_script)()
local function PPDYNAJ_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	game:GetService("UserInputService").InputBegan:Connect(function(key)
		if key.KeyCode == Enum.KeyCode.Insert then if script.Parent.Visible then script.Parent.Visible = false else script.Parent.Visible = true end end
	end)
end
coroutine.wrap(PPDYNAJ_fake_script)()
